{
  "name": "gran-publicador",
  "version": "0.5.0",
  "description": "Gran Publicador",
  "type": "module",
  "main": "dist/src/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "dotenv -e .env.development -- nest start --watch",
    "dev:debug": "dotenv -e .env.development -- nest start --debug --watch",
    "start:prod": "dotenv -e .env.production -- node dist/src/main.js",
    "start:prod:api": "dotenv -e .env.production -- node dist/src/main.js",
    "start:prod:worker": "dotenv -e .env.production -- node dist/src/worker.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ui": "pnpm --filter ui lint",
    "lint:ui:fix": "pnpm --filter ui lint:fix",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:prod:migrate": "dotenv -e .env.production -- prisma migrate deploy",
    "db:prod:migrate:deploy": "dotenv -e .env.production -- prisma migrate deploy",
    "db:prod:reset": "dotenv -e .env.production -- prisma migrate reset",
    "db:dev:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "db:dev:migrate:deploy": "dotenv -e .env.development -- prisma migrate deploy",
    "db:dev:push": "dotenv -e .env.development -- prisma db push",
    "db:dev:reset": "dotenv -e .env.development -- prisma migrate reset",
    "db:dev:studio": "dotenv -e .env.development -- prisma studio",
    "db:dev:seed": "dotenv -e .env.development -- prisma db seed",
    "db:dev:generate": "dotenv -e .env.development -- prisma generate",
    "test": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=16384' jest",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=16384' jest --selectProjects unit --runInBand",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=16384' jest --watch",
    "test:cov": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=16384' jest --coverage",
    "test:debug": "node --inspect-brk --loader ts-node/esm node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test:unit:debug": "node --inspect-brk --loader ts-node/esm node_modules/.bin/jest --runInBand --selectProjects unit --detectOpenHandles",
    "test:e2e:debug": "node --inspect-brk --loader ts-node/esm node_modules/.bin/jest --runInBand --selectProjects e2e --detectOpenHandles",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' dotenv -e .env.development -- jest --selectProjects e2e"
  },
  "dependencies": {
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.4.0",
    "@gran/shared": "workspace:*",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.12",
    "@nestjs/platform-socket.io": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/websockets": "^11.1.12",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "7.3.0",
    "@prisma/client-runtime-utils": "7.3.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "bullmq": "^5.70.0",
    "cache-manager": "^7.2.8",
    "cache-manager-redis-yet": "^5.1.5",
    "cacheable": "^1.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.1",
    "form-data": "^4.0.5",
    "grammy": "^1.39.3",
    "ioredis": "^5.9.2",
    "mdast-util-to-string": "^4.0.0",
    "nestjs-i18n": "^10.6.0",
    "nestjs-pino": "^4.5.0",
    "p-queue": "^9.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.17.2",
    "pino": "^10.3.0",
    "prisma": "7.3.0",
    "reflect-metadata": "^0.2.2",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "rxjs": "^7.8.2",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.8.3",
    "undici": "^7.19.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^30.2.0",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@types/form-data": "^2.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "nock": "^14.0.10",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "author": "Ivan K",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "pnpm@10.29.2",
  "pnpm": {
    "overrides": {
      "fastify": "^5.7.0"
    },
    "patchedDependencies": {
      "@webav/av-cliper@1.2.8": "patches/@webav__av-cliper@1.2.8.patch"
    }
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  }
}